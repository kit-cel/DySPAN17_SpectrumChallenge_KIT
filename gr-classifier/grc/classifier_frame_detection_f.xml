  </param>
  <sink>
    <name>in</name>
    <type>float</type>
    <nports>4</nports>
  </sink>
  <source>
    <name>out</name>
    <type>int</type>
    <nports>2</nports>
  </source>
  <source>
    <name>pu_frame_start</name>
    <type>message</type>
    <optional>True</optional>
  </source>
</block>
